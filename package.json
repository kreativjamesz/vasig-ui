{
  "name": "@vasig/ui",
  "version": "0.1.0",
  "description": "A clean, modern UI kit for Vue 3 + TypeScript projects",
  "type": "module",
  "main": "./dist/vasig-ui.umd.js",
  "module": "./dist/vasig-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vasig-ui.es.js",
      "require": "./dist/vasig-ui.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:lib": "vite build --mode library",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "typescript",
    "ui-library"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.2.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitepress": "^1.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0"
  }
}
